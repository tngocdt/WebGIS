<!DOCTYPE html>
<html>
    <head>
        <meta charset='utf8'>
        <title>MATERIALS STATUS</title>
        <link href="/css/styles.css" rel="stylesheet", type="text/css">

        
    </head>
    <body>
            <%= JSON.stringify(data) %>
        <script type="text/javascript">
            var resdata = <%- JSON.stringify(data) %>;
            var html = '<table class="table table-striped">';
            html += '<tr>';
            var flag = 0;
            $.each(resdata[0], function(index, value){
                '<th>'+index+'</th>';
            });
            html += '</tr>';
             $.each(resdata, function(index, value){
                 html += '<tr>';
                $.each(value, function(index2, value2){
                    html += '<td>'+value2+'</td>';
                });
                html += '<tr>';
             });
             html += '</table>';
             $('body').html(html);
        </script>
        
    </body>
</html>
    function openNav() {
        document.getElementById("mySidebar").style.width = "520px";
        document.getElementById("main").style.marginLeft = "520px";
        console.log(document.getElementById("mySidebar").style.width);
    }

    /* Set the width of the sidebar to 0 and the left margin of the page content to 0 */
    function closeNav() {
        console.log("Hide");
        document.getElementById("mySidebar").style.width = "0";
        document.getElementById("main").style.marginLeft = "0";
    }

    function ToggleNav() {
        var widthScreen = screen.width;
        var widthNav = document.getElementById("mySidebar").style.width;
        console.log(widthNav);

        if (widthNav == "0px") {
            if (widthScreen > 590) {
                document.getElementById("mySidebar").style.width = "520px";
                document.getElementById("main").style.marginLeft = "520px";
            } else {
                document.getElementById("mySidebar").style.width = "360px";
                document.getElementById("main").style.marginLeft = "360px";
            }
        } else {
            document.getElementById("mySidebar").style.width = "0";
            document.getElementById("main").style.marginLeft = "0";
        }
    }

    function GetSectorLine(strSectorGroup) {
        document.getElementById("idSectorLine").disabled = true;
        if (strSectorGroup.length == 0) {
            document.getElementById("idSectorLine").innerHTML = "";
            return;
        }

        var xmlhttp = new XMLHttpRequest();
        xmlhttp.onreadystatechange = function () {
            if (xmlhttp.readyState == XMLHttpRequest.DONE) {
                var njsdata = JSON.parse(xmlhttp.responseText);
                var $SectorLine = $("#idSectorLine");
                $SectorLine.empty();

                $SectorLine.append("<option value='Choose a Line'>Choose a Line</option>");
                for (var ii = 0; ii < njsdata.length; ii++) {
                    // alert(njsdata[ii].Sector);
                    var strSector = njsdata[ii].Sector;


                    $SectorLine.append("<option value='" + strSector + "'>" + strSector + "</option>");
                }
                // alert(njsdata + "\n" + njsdata.length);
                // document.getElementById("idSectorLine").innerHTML = xmlhttp.responseText;

            }
        }
        xmlhttp.open("GET", "api/getSectorLineByGroup?SectorGroup=" + strSectorGroup);
        xmlhttp.send(null);

        document.getElementById("idSectorLine").disabled = false;
    }

    function GetSectorStation(strSectorLine) {
        alert(strSectorLine);
        document.getElementById("idLineStation").disabled = true;
        if (strSectorLine.length == 0) {
            document.getElementById("idLineStation").innerHTML = "";
            return;
        }

        var xmlhttp = new XMLHttpRequest();
        xmlhttp.onreadystatechange = function () {
            if (xmlhttp.readyState == XMLHttpRequest.DONE) {
                var njsdata = JSON.parse(xmlhttp.responseText);
                var $LineStation = $("#idLineStation");
                $LineStation.empty();

                $LineStation.append("<option value='Choose a Station'>Choose a Station</option>");
                for (var ii = 0; ii < njsdata.length; ii++) {
                    // alert(njsdata[ii].Sector);
                    var strStationLocation = njsdata[ii].WorkStationLocation;


                    $LineStation.append("<option value='" + strStationLocation + "'>" + strStationLocation +
                        "</option>");
                }
                // alert(njsdata + "\n" + njsdata.length);
                // document.getElementById("idLineStation").innerHTML = xmlhttp.responseText;

            }
        }
        xmlhttp.open("GET", "api/getSectorStationByLine?SectorLine=" + strSectorLine);
        xmlhttp.send(null);

        document.getElementById("idLineStation").disabled = false;
    }

    function ShowOpionKBControl(strOpKBControl) {
        // alert(strOpKBControl);
        var idKBPartNumber = document.getElementById("idMaterialsNumberSec");
        var idKBPartLoc = document.getElementById("idMaterialsLocationSec");
        var idKBQty = document.getElementById("idMaterialsQtySec");

        if (strOpKBControl == "FlashingLocation") {
            idKBPartNumber.style.display = "none";
            idKBPartLoc.style.display = "block";
            idKBQty.style.display = "none";
        } else if (strOpKBControl == "UpdateKBQty") {
            idKBPartNumber.style.display = "block";
            idKBPartLoc.style.display = "block";
            idKBQty.style.display = "block";

        }
    }
</script>
    <script>
        function validateForm() {
            var valMaterialsNumber = document.forms["frmKBStatusControl"]["nMaterialsNumber"].value;
            if (valMaterialsNumber.length != 9) {
                alert("VUI LONG SCAN DUNG SO MA PARTNUMBER!!!");
                document.getElementById("idMaterialsNumber").focus();
                document.getElementById("idMaterialsNumber").select();
                return false;
            }

            var valMaterialsLocation = document.forms["frmKBStatusControl"]["nMaterialsLocation"].value;
            var intPosition = valMaterialsLocation.indexOf("-");
            if ((valMaterialsLocation.length < 9) || (intPosition == -1)) {
                alert("VUI LONG SCAN DUNG MA SO VI TRI!!!");
                document.getElementById("idMaterialsLocation").focus();
                document.getElementById("idMaterialsLocation").select();
                return false;
            }
            
            var valMaterialsQty = document.forms["frmKBStatusControl"]["nMaterialsQty"].value;
            if (isNaN(valMaterialsQty)){
                alert("VUI LONG SCAN SO LUONG LINH KIEN!!!");
                document.getElementById("idMaterialsQty").focus();
                document.getElementById("idMaterialsQty").select();
                return false;
            }
            else{
                if (Number(valMaterialsQty) <= 0){
                    alert("VUI LONG SCAN SO LUONG LINH KIEN!!!");
                    document.getElementById("idMaterialsQty").focus();
                document.getElementById("idMaterialsQty").select();
                    return false;
                }
            }
        }
    </script>

